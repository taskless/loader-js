    "husky": "^9.0.11",
    "lint-staged": "^15.2.4",
    "prettier": "^3.2.5",
    "shx": "^0.3.4",
    "syncpack": "^12.3.2",
    "tsx": "^4.16.5",
    "type-fest": "^4.23.0",
    "typescript": "^5.4.5",
    "vite": "^5.4.2",
    "vite-plugin-dts": "^4.0.3",
    "vite-plugin-externalize-deps": "^0.8.0",
    "vite-tsconfig-paths": "^5.0.1",
    "vitest": "^2.0.5",
    "xo": "^0.59.3"
